/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
page{
  background-color: #fff;
}

.nav {
  width: 100%;
  height: 170rpx;
  position: fixed;
  left: 0rpx;
  top: 0rpx;
  z-index: 9999;
}

.nav-clone {
  width: 100%;
  height: 170rpx;
}

.nav .nav-bgc {
  width: 100%;
  height: 100%;
}

.nav-logo {
  width:470rpx;
  height:100rpx;
  position:absolute;
  left:50%;
  top:64%;
  transform:translate(-50%, -50%);
}

.nav-back {
  position: absolute;
  left: 0%;
  top: 60%;
  transform: translateY(-50%);
  width: 80rpx;
  height: 80rpx;
  padding: 20rpx;
  box-sizing: border-box;
}

.nav-back image {
  width: 100%;
  height: 100%;
}

.content {
  width: 100%;
  padding: 20rpx 30rpx;
  box-sizing: border-box;
}

.content-header {
  border-radius: 10rpx;
  overflow: hidden;
  width: 65%;
  height: 120rpx;
  margin: 0 auto;
  margin-top: 100rpx;
  margin-bottom: 135rpx;
  z-index: 999;
}

.content-header image {
  width: 100%;
  height: 100%;
}

.content-detail {
  width: 100%;
  margin-bottom: 150rpx;
}

.content-detail-li {
  width: 90%;
  position: relative;
  padding-left: 70rpx;
  box-sizing: border-box;
  margin: 0 auto;
}

.content-detail-li image {
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  left: 0rpx;
  top: 0rpx;
}

.content-detail-li view {
  color: #ff8a02;
  font-size: 34rpx;
}

.content-detail-li-other {
  margin-top: 50rpx;
}

.content-detail-li-other view {
  color: #a7a8a3;
  font-size: 34rpx;
}

/* 底部 */
.content-footer {
  width: 90%;
  margin: 0 auto;
  text-align: center;
  color: #fff;
  font-size: 38rpx;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  box-sizing: border-box;
  border-radius: 10rpx;
  font-weight: bolder;
  line-height: 1.3;
  background: -webkit-linear-gradient(left, rgb(254, 143, 16), rgb(249, 187, 29));
  background: -o-linear-gradient(right, rgb(254, 143, 16), rgb(249, 187, 29));
  background: -moz-linear-gradient(right, rgb(254, 143, 16), rgb(249, 187, 29));
  background: linear-gradient(to right, rgb(254, 143, 16), rgb(249, 187, 29));
}
