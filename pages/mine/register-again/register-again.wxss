page{
  background-color: #f3f3f3;
}
.nav{
  width: 100%;
  height: 170rpx;
  position: fixed;
  left: 0rpx;
  top: 0rpx;
  z-index: 9999;
}
.nav-clone{
  width: 100%;
  height: 170rpx;
}
.nav .nav-bgc{
  width: 100%;
  height: 100%;
}
.nav-title{
  font-size: 40rpx;
  color: #fff;
  margin: 0 auto;
  position: absolute;
  left: 50%;
  top: 60%;
  transform: translate(-50%, -50%);
  font-weight: bold;
}
.nav-back{
  position:absolute;
  left:0%;
  top:60%;
  transform:translateY(-50%);
  width:80rpx;
  height:80rpx;
  padding:20rpx;
  box-sizing:border-box;
}
.nav-back image{
  width: 100%;
  height: 100%;
}
.regmem{
  padding: 100rpx 30rpx;
  box-sizing: border-box;
}
.regmem-box{
  border: 2rpx solid #f29509;
  border-radius: 10rpx;
  width: 100%;
  background-color: #fff;
}
.regmem-box-header{
  width: 100%;
  height: 30rpx;
  margin-bottom: 125rpx;
  background: -webkit-linear-gradient(left, rgb(254,143,16) , rgb(249,187,29));
  background: -o-linear-gradient(right, rgb(254,143,16) , rgb(249,187,29));
  background: -moz-linear-gradient(right, rgb(254,143,16) , rgb(249,187,29));
  background: linear-gradient(to right, rgb(254,143,16) , rgb(249,187,29));
}
.regmem-box-content{
  width: 85%;
  margin: 0 auto;
  background-color: #fff;
}
.box-content-one{
  display: flex;
  width: 100%;
  line-height: 60rpx;
}
.content-one-image{
  width: 45rpx;
  height: 45rpx;
  margin-top: 8rpx;
}
.content-one-name{
  font-size: 36rpx;
  margin-left: 30rpx;
  margin-right: 10rpx;
  width: 25%;
  text-align: justify;
}
.content-one-name>text {
  display: inline-block;
  padding-left: 100%;
}
.content-one-picker{
  width: 60%;
  border: 2rpx solid #cba751;
  border-radius: 10rpx;
  font-size: 34rpx;
  color: #f47f06;
  text-align: center;
  line-height: 55rpx;
}
.content-one-price{
  width: 60%;
  border: 2rpx solid #cba751;
  border-radius: 10rpx;
  font-size: 34rpx;
  color: #f47f06;
  text-align: center;
  height: 60rpx;
  line-height: 40rpx;
  padding: 10rpx;
  box-sizing: border-box;
}
.box-content-one-other{
  margin-top: 95rpx;
}
.regmem-box-btn{
  width: 83%;
  border-radius: 10rpx;
  background: -webkit-linear-gradient(left, rgb(254,143,16) , rgb(249,187,29));
  background: -o-linear-gradient(right, rgb(254,143,16) , rgb(249,187,29));
  background: -moz-linear-gradient(right, rgb(254,143,16) , rgb(249,187,29));
  background: linear-gradient(to right, rgb(254,143,16) , rgb(249,187,29));
  color: #fff;
  font-size: 36rpx;
  padding: 20rpx;
  margin: 0 auto;
  text-align: center;
  margin-top:240rpx;
  margin-bottom:280rpx;
}