.nav{
    width: 100%;
    height: 170rpx;
    position: fixed;
    left: 0rpx;
    top: 0rpx;
    z-index: 9999;
  }
  .nav-clone{
    width: 100%;
    height: 170rpx;
  }
  .nav .nav-bgc{
    width: 100%;
    height: 100%;
  }
  .nav-title{
    font-size: 40rpx;
    color: #fff;
    margin: 0 auto;
    position: absolute;
    left: 50%;
    top: 60%;
    transform: translate(-50%, -50%);
    font-weight: bold;
  }
  .nav-back{
    position:absolute;
    left:0%;
    top:60%;
    transform:translateY(-50%);
    width:80rpx;
    height:80rpx;
    padding:20rpx;
    box-sizing:border-box;
  }
  .nav-back image{
    width: 100%;
    height: 100%;
  }
  /*  */
  .translate{
    width: 100%;
  }
  .translate-li{
    width: 100%;
    padding: 20rpx 30rpx;
    box-sizing: border-box;
    font-size: 36rpx;
    border-bottom: 2rpx solid #f3f3f3;
  }
  .translate-li-top{
    width: 100%;
    display: flex;
    justify-content: space-between;
    margin-top: 10rpx;
  }
  .translate-li-top-other{
    font-size: 30rpx;
    color: grey;
  }
  .showNot{
    text-align: center;
    color: #b7b2b9;
    font-weight: bold;
    font-size: 40rpx;
    margin-top: 300rpx;
  }