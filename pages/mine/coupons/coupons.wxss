page{
}
.nav{
  width: 100%;
  height: 170rpx;
  position: fixed;
  left: 0rpx;
  top: 0rpx;
  z-index: 9999;
}
.nav-clone{
  width: 100%;
  height: 170rpx;
}
.nav .nav-bgc{
  width: 100%;
  height: 100%;
}
.nav-title{
  font-size: 40rpx;
  color: #fff;
  margin: 0 auto;
  position: absolute;
  left: 50%;
  top: 60%;
  transform: translate(-50%, -50%);
  font-weight: bold;
}
.nav-back{
  position:absolute;
  left:0%;
  top:60%;
  transform:translateY(-50%);
  width:80rpx;
  height:80rpx;
  padding:20rpx;
  box-sizing:border-box;
}
.nav-back image{
  width: 100%;
  height: 100%;
}
.coupons{
  width: 100%;
  padding: 50rpx 30rpx;
  box-sizing: border-box;
}
.coupons-li{
  width:93%;
  height:145rpx;
  margin:0 auto;
  position: relative;
  margin-bottom: 50rpx;
  margin-top: 10rpx;
}
.coupons-li image{
  width: 100%;
  height: 100%;
}
.coupons-title{
  font-size: 32rpx;
  color: #fff;
  position: absolute;
  left: 20%;
  top: 20%;
}
.coupons-time{
  font-size: 26rpx;
  color: #fff;
  position: absolute;
  left: 20%;
  bottom: 10%;
}
.coupons-btn{
  font-size: 38rpx;
  color: #fff;
  position: absolute;
  right: 5%;
  top: 30%;
  font-weight: bold;
}
.coupons-price{
  font-size:32rpx;
  color:#fff;
  position:absolute;
  right:30%;
  top:7%;
}
.coupons-price>text{
  font-size: 50rpx;
}
.coupons-no{
  color: #9f9f9f;
  font-size: 30rpx;
  width: 100%;
  text-align: center;
}
.coupons-no>text{
  color: #ff6d00;
}
.nomoreCoupons{
  text-align: center;
  color: #b7b2b9;
  font-weight: bold;
  font-size: 40rpx;
  margin-top: 300rpx;
}
.coupons-no-other{
  position: fixed;
  bottom: 50rpx;
  left: 50%;
  transform: translateX(-50%);
  color: #9f9f9f;
  font-size: 30rpx;
  width: 100%;
  text-align: center;
}
.coupons-no-other>text{
  color: #ff6d00;
}
/* 没有礼券 */
.noCoupons{
  position: absolute;
  left: 50%;
  top: 45%;
  transform: translate(-50%, -50%);
  width: 50%;
  text-align: center;
}
.noCoupons image{
  width:200rpx;
  height:200rpx;
  margin-bottom:60rpx;
}
.noCoupons-view{
  font-size: 32rpx;
  font-weight: bold;
  color: #5e5e5e;
}
