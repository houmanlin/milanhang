page{
  background-color: #fff;
}
.nav{
  width: 100%;
  height: 170rpx;
  position: fixed;
  left: 0rpx;
  top: 0rpx;
  z-index: 9999;
}
.nav-clone{
  width: 100%;
  height: 170rpx;
}
.nav .nav-bgc{
  width: 100%;
  height: 100%;
}
.nav-title{
  font-size: 40rpx;
  color: #fff;
  margin: 0 auto;
  position: absolute;
  left: 50%;
  top: 60%;
  transform: translate(-50%, -50%);
  font-weight: bold;
}
.nav-back{
  position:absolute;
  left:0%;
  top:60%;
  transform:translateY(-50%);
  width:80rpx;
  height:80rpx;
  padding:20rpx;
  box-sizing:border-box;
}
.nav-back image{
  width: 100%;
  height: 100%;
}
.member{
  width: 100%;
  padding: 70rpx 50rpx 0rpx 50rpx;
  box-sizing: border-box;
}
.member-header{
  width: 100%;
  height: 400rpx;
  margin-bottom: 50rpx;
  position: relative;
}
.yaoqing{
  position: absolute;
  bottom: 10%;
  left: 0rpx;
  width: 100%;
  text-align: center;
  color: #fff;
  font-weight: bold;
  font-size: 36rpx;
}
.member-header image{
  width: 100%;
  height: 100%;
}
.member-content{
  width: 100%;
  margin-top: 20rpx;
  /* margin-bottom: 20rpx; */
  /* margin-bottom: 300rpx; */
}
.member-content-title{
  display: flex;
  padding-left:20rpx;
  box-sizing:border-box;
  margin-bottom: 20rpx;
}
.content-title-img{
  width: 40rpx;
  height: 45rpx;
}
.content-title-title{
  color: #353535;
  font-size: 30rpx;
  margin-left: 40rpx;
}
.content-title-neirong{
  /* width: 70%; */
  width: 100%;
  margin: 0 auto;
  font-size: 28rpx;
  color: #353535;
  line-height: 50rpx;
}
.content-title-neirong>image{
  width: 100%;
  /* height: 300rpx; */
  margin-bottom: 30rpx;
}
.content-title-neirong text{
  margin-top: 30rpx;
}
.member-bottom{
  position:fixed;
  left:50%;
  transform:translateX(-50%);
  bottom:5%;
  width:87%;
  background:-webkit-linear-gradient(left, rgb(254,143,16) , rgb(249,187,29));
  background:-o-linear-gradient(right, rgb(254,143,16) , rgb(249,187,29));
  background:-moz-linear-gradient(right, rgb(254,143,16) , rgb(249,187,29));
  background:linear-gradient(to right, rgb(254,143,16) , rgb(249,187,29));
  font-size:34rpx;
  font-weight:bolder;
  color:#fff;
  border-radius:10rpx;
  padding:20rpx;
  box-sizing:border-box;
  text-align:center;
  /* margin: 0 auto; */
}
.bottom{
  width: 100%;
  height: 300rpx;
}