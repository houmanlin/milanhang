page{
  background-color: #f3f3f3;
}
.nav{
  width: 100%;
  height: 170rpx;
  position: fixed;
  left: 0rpx;
  top: 0rpx;
  z-index: 9999;
}
.nav-clone{
  width: 100%;
  height: 170rpx;
}
.nav .nav-bgc{
  width: 100%;
  height: 100%;
}
.nav-title{
  font-size: 40rpx;
  color: #fff;
  margin: 0 auto;
  position: absolute;
  left: 50%;
  top: 60%;
  transform: translate(-50%, -50%);
  font-weight: bold;
}
.nav-back{
  position:absolute;
  left:0%;
  top:60%;
  transform:translateY(-50%);
  width:80rpx;
  height:80rpx;
  padding:20rpx;
  box-sizing:border-box;
}
.nav-back image{
  width: 100%;
  height: 100%;
}
/* 下方信息 */
.bank{
  width: 100%;
}
.bank-top-li{
  display: flex;
  background-color: #fff;
  padding: 20rpx 30rpx;
  box-sizing: border-box;
  border-bottom: 2rpx solid #e9e9e9;
}
.bank-top-li:nth-child(3){
  border-bottom: none;
}
.bt-li-left{
  width: 30%;
  font-size: 32rpx;
}
.bt-li-right{
  width: 65%;
  color: #7c7c7c;
  font-size: 30rpx;
  line-height: 50rpx;
}
.bank-content{
  color: #7e7e7e;
  margin-top: 20rpx;
  margin-bottom: 50rpx;
  padding-left: 30rpx;
  font-size: 28rpx;
}
.login-footer{
  margin-top: 40rpx;
  width: 90%;
  margin: 0 auto;
}
.login-footer view{
  width:100%;
  text-align:center;
  color:#fff;
  font-size:38rpx;
  padding-top:20rpx;
  padding-bottom:20rpx;
  box-sizing:border-box;
  border-radius:10rpx;
  font-weight:bolder;
  background: -webkit-linear-gradient(left, rgb(254,143,16) , rgb(249,187,29));
  background: -o-linear-gradient(right, rgb(254,143,16) , rgb(249,187,29));
  background: -moz-linear-gradient(right, rgb(254,143,16) , rgb(249,187,29));
  background: linear-gradient(to right, rgb(254,143,16) , rgb(249,187,29));
}
.bonus-content-three{
  color: #fe8f10;
  font-size: 32rpx;
  width: 100%;
  text-align: right;
  margin-top: 2rpx;
  padding: 20rpx 30rpx;
  box-sizing: border-box;
  background-color: #fff;
}