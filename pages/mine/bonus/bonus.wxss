page{
  background-color: #f3f3f3;
}
.nav{
  width: 100%;
  height: 170rpx;
  position: fixed;
  left: 0rpx;
  top: 0rpx;
  z-index: 9999;
}
.nav-clone{
  width: 100%;
  height: 170rpx;
}
.nav .nav-bgc{
  width: 100%;
  height: 100%;
}
.nav-title{
  font-size: 40rpx;
  color: #fff;
  margin: 0 auto;
  position: absolute;
  left: 50%;
  top: 60%;
  transform: translate(-50%, -50%);
  font-weight: bold;
}
.nav-back{
  position:absolute;
  left:0%;
  top:60%;
  transform:translateY(-50%);
  width:80rpx;
  height:80rpx;
  padding:20rpx;
  box-sizing:border-box;
}
.nav-back image{
  width: 100%;
  height: 100%;
}
.bonus{
  width: 100%;
}
.bonus-top{
  position: relative;
  background-color: #fff;
  margin-top: 10rpx;
  margin-bottom: 10rpx;
  display: flex;
  padding: 20rpx 30rpx;
  box-sizing: border-box;
}
.image-one{
  width: 60rpx;
  height: 60rpx;
  margin-right: 20rpx;
}
.bank-title{
  font-size: 30rpx;
}
.image-two{
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  right: 5%;
  top: 50%;
  transform: translateY(-50%);
}
/* 人数显示 */
.bonus-show{
  width: 100%;
  margin-bottom: 10rpx;
  background-color: #fff;
  display: flex;
  justify-content: space-between;
  border-top: 2rpx solid #d9d9d9;
  border-bottom: 2rpx solid #d9d9d9;
}
.bonus-show-li{
  width: 50%;
  text-align: center;
  padding: 30rpx;
  box-sizing: border-box;
}
.bonus-show-li:nth-child(1){
  border-right: 2rpx solid #d9d9d9;
}
.bs-li-top{
  font-size: 34rpx;
  color: #7b7b7b;
  margin-bottom: 20rpx;
}
.bs-li-bottom{
  width: 100%;
  text-align: center;
  font-size: 34rpx;
}
.bs-li-bottom image{
  width: 35rpx;
  height: 35rpx;
  margin-right: 10rpx;
}
/* 下方 */
.bonus-content{
  width: 100%;
  background-color: #fff;
  padding: 50rpx 20rpx;
  box-sizing: border-box;
  margin-bottom: 30rpx;
}
.bonus-content-top{
  font-size: 32rpx;
  color: black;
  margin-bottom: 40rpx;
}
.bonus-content-top>text{
  font-size: 26rpx;
}
.bonus-content-two{
  width: 100%;
  border-bottom: 2rpx solid #f4f4f4;
  font-size: 38rpx;
  display: flex;
  margin-top: 40rpx;
}
.bonus-content-three{
  color: #fe8f10;
  font-size: 32rpx;
  width: 100%;
  text-align: right;
  margin-top: 30rpx;
}
.login-footer{
  margin-top: 40rpx;
  width: 90%;
  margin: 0 auto;
}
.login-footer view{
  width:100%;
  text-align:center;
  color:#fff;
  font-size:38rpx;
  padding-top:20rpx;
  padding-bottom:20rpx;
  box-sizing:border-box;
  border-radius:10rpx;
  font-weight:bolder;
  background: -webkit-linear-gradient(left, rgb(254,143,16) , rgb(249,187,29));
  background: -o-linear-gradient(right, rgb(254,143,16) , rgb(249,187,29));
  background: -moz-linear-gradient(right, rgb(254,143,16) , rgb(249,187,29));
  background: linear-gradient(to right, rgb(254,143,16) , rgb(249,187,29));
}