page{
  background-color: #f3f3f3;
}
.my-header{
  width: 100%;
  height: 320rpx;
  position: relative;
}
.my-header-bgc{
  width: 100%;
  height: 100%;
}
.my-header-box{
  position:absolute;
  left:33rpx;
  top:192rpx;
  border:2rpx solid rgb(226, 226, 226);
  border-radius:10rpx;
  padding:12rpx 20rpx;
  box-sizing: border-box;
  width: 92%;
  display: flex;
  justify-content: space-between;
  background-color: #fff;
  z-index: 100;
}
.header-box-author{
  width:170rpx;
  height:170rpx;
  border-radius:50%;
  margin-top:30rpx;
  margin-left:12rpx;
  margin-bottom: 20rpx;
  overflow:hidden;
}
.authorDetail-one-name{
  width: 60%;
}
.header-box-author image{
  width: 100%;
  height: 100%;
}
.header-box-authorDetail{
  padding:60rpx 0rpx 30rpx 0rpx;
  box-sizing:border-box;
  color:#000000;
  font-family: "苹方黑体";
  font-weight:bold;
  margin-left:30rpx;
  width:64%;
  font-size:32rpx;
}
.box-authorDetail-one{
  position: relative;
}
.authorDetail-one-img{
  position:absolute;
  right:0rpx;
  top:10rpx;
  width:120rpx;
  height:35rpx;
}
.authorDetail-one-img-other{
  position:absolute;
  right:0rpx;
  top:0rpx;
  width:80rpx;
  height:60rpx;
}
.authorDetail-one-img-other image{
  position:absolute;
  right:0rpx;
  top:0rpx;
  width:80rpx;
  height:60rpx;
}
.authorDetail-one-img image{
  width: 100%;
  height: 100%;
}
.ifMember{
  margin-top: 40rpx;
  position: relative;
  color: #ff7f0d
}
.ifMember-time{
  font-size: 24rpx;
}
.ifMember-time-other{
  color: #9aa1a9;
}
.ifMember-btn{
  position: absolute;
  background-color: #ff2d2d;
  color: #fff;
  font-size: 24rpx;
  border-radius: 10rpx;
  padding: 5rpx 20rpx;
  right:0rpx;
  bottom:0rpx;
}
/* 中间按钮部分 */
.my-content{
  width: 92%;
  border:2rpx solid rgb(226, 226, 226);
  background-color: #fff;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  box-sizing: border-box;
  border-radius: 10rpx;
  margin: 0 auto;
  margin-top: 147rpx;
  height: auto;
  display: flex;
  flex-wrap:wrap; 
  justify-content: left
}
.callServiceBtn, .my-content image{
  width: 32%;
  height: 176rpx;
   padding: 0!important;

}
.nth{
 
  width: 36%!important;

}
.callServiceBtn::after{
  border-radius:0 ;
  border: none;
  background: rgba(0, 0, 0, 0) 
}

.callService{
  width: 100%!important;
  height: 100%!important;

}

.content-first{
  width: 100%;
  display: flex;
  justify-content: space-between;
  margin-bottom: 155rpx;
}
.content-first-one{
  width:27%;
  height:140rpx;
  text-align: center;
}
.first-one-title{
  font-size: 30rpx;
  color: #6a6d72;
  font-weight: bold;
  margin-top: 15rpx;
}
.content-first-one image{
  width: 93%;
  height: 93%;
}
.content-second{
  width: 100%;
  display: flex;
}
.content-first-other{
  margin-right: 10%;
}
.erweima{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.3);
  z-index: 999;

}
.erweima image{
  width: 416rpx;
  height: 416rpx;
  display: inline-block;
  margin-top: 50%; 
  margin-left: 50%;
  position: relative;
  left: -25%
}