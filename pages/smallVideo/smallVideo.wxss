.nav {
  width: 100%;
  height: 170rpx;
  position: fixed;
  left: 0rpx;
  top: 0rpx;
  z-index: 9999;
}

.nav-clone {
  width: 100%;
  height: 170rpx;
}

.nav .nav-bgc {
  width: 100%;
  height: 100%;
}

.nav-logo {
  width:470rpx;
  height:100rpx;
  position:absolute;
  left:50%;
  top:64%;
  transform:translate(-50%, -50%);
}
.nav-back{
  position:absolute;
  left:0%;
  top:60%;
  transform:translateY(-50%);
  width:80rpx;
  height:80rpx;
  padding:20rpx;
  box-sizing:border-box;
}
.nav-back image{
  width: 100%;
  height: 100%;
}
/* tab切换 */

.order-search-third {
  margin-top: 15rpx;
  box-shadow: 0 1px 1px rgba(0, 0, 0, .1);
}

.scroll-box {
  width: 100%;
  background-color: #fff;
  white-space: nowrap;
  height: 90rpx;
  line-height: 90rpx;
}

.scroll-box-blank {
  display: inline-block;
  width: 24rpx;
}

.scrollClick {
  display: inline-block;
  height: 100%;
  box-sizing: border-box;
  padding: 0rpx 10rpx;
  color: #ff7f0d;
  margin-right: 20rpx;
  margin-left: 20rpx;
  font-size: 36rpx;
  text-align: center;
  overflow: hidden;
  text-overflow: ellipsis;
  background-color: #ffffff;
  border-bottom: 4rpx solid #ff7f0d;
  font-weight: bold;
}

.scrollNoClick {
  box-sizing:border-box;
  display:inline-block;
  padding:0rpx 10rpx;
  color:#181818;
  margin-right:20rpx;
  background-color:#ffffff;
  margin-left:20rpx;
  font-size:36rpx;
  text-align:center;
  overflow:hidden;
  text-overflow:ellipsis;
  font-weight: bold;
}
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

/* 下方视频部分 */
.video-father{
  width: 100%;
  padding-top:15rpx;
  box-sizing:border-box;
}
.video-border{
  border-bottom: 2rpx solid #dfdfdf;
  width: 100%;
  padding: 0rpx 40rpx;
  box-sizing: border-box;
}
.video-box{
  width:100%;
  background-color:#fff;
  /* margin-bottom:2%; */
  overflow:hidden;
  padding-top: 30rpx;
}
.video-item{
  position: relative;
  width: 100%;
  height: 400rpx;
  overflow: hidden;
}
video{
  width: 100%;
  height: 100%;
}
.video-cover{
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
}
.video-play-btn{
  position: absolute;
  width: 100rpx;
  height: 100rpx;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  margin: auto
}
.video-detail{
  width: 100%;
  padding: 20rpx 0rpx;
  box-sizing: border-box;
  background-color: #f4f6f5;
}
.video-detail-title{
  color: #06042a;
  font-size: 30rpx;
  font-family: 'Times New Roman', Times, serif
}
.video-detail-title>text{
  font-weight: bold;
  color: #ff7f0d;
}

.showNot{
  text-align: center;
  color: #b7b2b9;
  font-weight: bold;
  font-size: 40rpx;
  margin-top: 300rpx;
}
