
page{
  background-color: #eaebec;
}
.nav{
  width: 100%;
  height: 170rpx;
  position: fixed;
  left: 0rpx;
  top: 0rpx;
  z-index: 9999;
}
.nav-clone{
  width: 100%;
  height: 170rpx;
}
.nav .nav-bgc{
  width: 100%;
  height: 100%;
}
.nav-title{
  font-size: 40rpx;
  color: #fff;
  margin: 0 auto;
  position: absolute;
  left: 50%;
  top: 60%;
  transform: translate(-50%, -50%);
  font-weight: bold;
}
.nav-back{
  position:absolute;
  left:0%;
  top:60%;
  transform:translateY(-50%);
  width:80rpx;
  height:80rpx;
  padding:20rpx;
  box-sizing:border-box;
}
.nav-back image{
  width: 100%;
  height: 100%;
}
/* 搜索 */
.search{
  background-color: #fff;
}
.search-top{
  display: flex;
  justify-content: space-between;
  border-bottom: 2rpx solid #d3d3d3;
  padding: 10rpx 20rpx;
  box-sizing: border-box;
  margin-bottom: 20rpx;
}
.search-top-input{
  width: 80%;
  padding-left: 20rpx;
  box-sizing: border-box;
  color: #c3c3c3;
  font-size: 34rpx;
}
.input-placeholder{
  color: #c3c3c3;
}
.search-top-btn{
  font-size: 34rpx;
  width: 15%;
  text-align: center;
}
.search-content{
  padding: 20rpx 0rpx 20rpx 30rpx;
  box-sizing: border-box;
}
.sc-top{
  font-size: 30rpx;
  color: #747474;
}
.sc-content{
  margin-top: 20rpx;
  border-top: 2rpx solid #d3d3d3;
}
/*  */
.actBox{
  padding: 0rpx 20rpx;
  box-sizing: border-box;
}

.activity-box{
  width: 100%;
  display: flex;
  margin-bottom: 10rpx;
  padding-bottom: 20rpx;
}
.ab-image{
  width:50%;
  height:220rpx;
  margin-right:30rpx;
  overflow: hidden;
  border-radius: 10rpx;
}
.ab-image .ab-image-one{
  width: 100%;
  height: 100%;
}
.ab-image-two{
  width:70rpx;
  height:40rpx;
  position:absolute;
  left:5%;
  top:5%;
  z-index:999;
}
.ab-detail{
  width: 50%;
  padding-top: 25rpx;
  box-sizing: border-box;
}
.ab-detail-one{
  color: #0a0a0a;
  font-size: 28rpx;
  margin-bottom: 10rpx;
  font-weight: bold;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  width: 100%;
}
.ab-detail-two{
  color: #797979;
  font-size: 22rpx;
  margin-bottom: 10rpx;
}
.ab-detail-three{
  color: #797979;
  font-size: 22rpx;
  margin-bottom: 10rpx;
}
.ab-detail-three>text{
  font-size: 24rpx;
}
.ab-detail-four{
  font-size: 18rpx;
  color: #cf595f;
  position: relative;
  width: 95%;
}
.ad-four>.text1{
  font-size: 30rpx;
}
.ad-four-two{
  position: absolute;
  right: 0rpx;
  bottom: 0rpx;
}