page{
  background-color: #fff;
}
.nav{
  width: 100%;
  height: 170rpx;
  position: fixed;
  left: 0rpx;
  top: 0rpx;
  z-index: 9999;
}
.nav-clone{
  width: 100%;
  height: 170rpx;
}
.nav .nav-bgc{
  width: 100%;
  height: 100%;
}
.nav-title{
  font-size: 40rpx;
  color: #fff;
  margin: 0 auto;
  position: absolute;
  left: 50%;
  top: 60%;
  transform: translate(-50%, -50%);
  font-weight: bold;
}
.nav-back{
  position:absolute;
  left:0%;
  top:60%;
  transform:translateY(-50%);
  width:80rpx;
  height:80rpx;
  padding:20rpx;
  box-sizing:border-box;
}
.nav-back image{
  width: 100%;
  height: 100%;
}
/* tab切换 */
.tabAll{
  position: fixed;
  background-color: #fff;
  width: 100%;
  z-index: 9999;
}
.tabAll-clone{
  width: 100%;
  height: 85rpx;
}
.tabAll-top{
  display: flex;
  justify-content: space-around;
  width: 100%;
  padding: 15rpx;
  box-sizing: border-box;
  z-index: 999;
  position: absolute;
  left: 0rpx;
  top: 0rpx;
  background-color: #fff;
  border-bottom: 2rpx solid #dbdbdb;
}
.tt-li{
  position: relative;
  width: 33%;
  padding: 0rpx 30rpx;
  box-sizing: border-box;
  color: #3b3b3b;
  font-size: 30rpx;
  border-right: 2rpx solid #dbdbdb;
}
.tt-li>view{
  width: 75%;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.tt-li:nth-child(3){
  border-right: none;
}
.tt-li>image{
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  right: 5%;
  top: -3rpx;
}
.mask{
  position: fixed;
  left: 0rpx;
  top: 0rpx;
  bottom: 0rpx;
  right: 0rpx;
  background-color: #000000;
  opacity: 0.4;
  z-index: 99;
}
/* 综合排序框 */
.ttli-box{
  position: absolute;
  width: 100%;
  left: 0rpx;
  top: 70rpx;
  background-color: #fff;
}
.ttb-li{
  padding:20rpx;
  box-sizing: border-box;
  font-size: 30rpx;
}
/* 中间分类 */
.ttli-box-three{
  position: absolute;
  width: 100%;
  left: 0rpx;
  top: 70rpx;
  background-color: #fff;
}
.container {
  position: relative;
  width: 100%;
  height: 400rpx;
  background-color: #fff;
  color: #939393;
  box-sizing: border-box;
}
.nav_left {
  display:inline-block;
  width:25%;
  height:100%;
  position:absolute;
  top:0rpx;
  left:0;
  background:#eeeeee;
  text-align:center;
  color:#7e7e7e;
  overflow-y: scroll;
  -webkit-overflow-scrolling: touch;
}
/*左侧栏list的item*/
.nav_left .nav_left_items {
  height: 20px;
  line-height: 20px;
  padding: 6px 0;
  border-bottom: 1px solid #dedede;
  font-size: 28rpx;
}

/*左侧栏list的item被选中时*/
.nav_left .nav_left_items.active {
  background: #fff;
  color: #fe8f10;
}
.nav_right{
  position: absolute;
  top: 0;
  right: 0;
  flex: 1;
  width: 75%;
  height: 100%;
  padding: 0rpx 10rpx 10rpx 10px;
  box-sizing: border-box;
  background: white;
  height: 100%;
  overflow-y: scroll;
  -webkit-overflow-scrolling: touch;
}
.nav_right_title{
  padding: 10rpx 20rpx;
  box-sizing: border-box;
  background-color: #f3f3f3;
  margin-bottom: 2rpx;
  font-size: 28rpx;
}
/* 筛选 */
.ttli-box-two{
  position: absolute;
  width: 100%;
  left: 0rpx;
  top: 70rpx;
  display: flex;
  justify-content: space-around;
  flex-wrap: wrap;
  background-color: #fff;
  padding-top: 10rpx;
  box-sizing: border-box;
}
.ttbt-li{
  width: 27%;
  text-align: center;
  font-size: 30rpx;
  padding: 10rpx 20rpx;
  box-sizing: border-box;
  margin-bottom: 30rpx;
  background-color: #efeff4;
  border-radius: 10rpx;
}
/* 下方内容 */
.curriculum{
  width: 100%;
}
.actBox{
  padding: 20rpx;
  box-sizing: border-box;
}

.activity-box{
  width: 100%;
  margin-bottom: 10rpx;
  border-bottom: 2rpx solid #9ba2aa;
  padding: 10rpx 10rpx 20rpx 10rpx;
  box-sizing: border-box;
}

.acbox{
  width: 100%;
  display: flex;
}
.ab-image{
  width:50%;
  height:220rpx;
  margin-right:30rpx;
  overflow: hidden;
  border-radius: 10rpx;
  position: relative;
}
.ab-image .ab-image-one{
  width: 100%;
  height: 100%;
}
.ab-image-two{
  width:70rpx;
  height:40rpx;
  position:absolute;
  left:5%;
  top:5%;
  z-index:999;
}
.ab-detail{
  width: 50%;
  padding-top: 25rpx;
  box-sizing: border-box;
}
.ab-detail-one{
  color: #0a0a0a;
  font-size: 28rpx;
  margin-bottom: 10rpx;
  font-weight: bold;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  width: 100%;
}
.ab-detail-two{
  color: #797979;
  font-size: 22rpx;
  margin-bottom: 10rpx;
}
.ab-detail-three{
  color: #797979;
  font-size: 22rpx;
  margin-bottom: 10rpx;
}
.ab-detail-three>text{
  font-size: 24rpx;
}
.ab-detail-four{
  font-size: 18rpx;
  color: #cf595f;
  position: relative;
  width: 95%;
}
.ad-four>.text1{
  font-size: 30rpx;
}
.ad-four-two{
  position: absolute;
  right: 0rpx;
  bottom: 0rpx;
}
.showNot{
  text-align: center;
  color: #b7b2b9;
  font-weight: bold;
  font-size: 40rpx;
  margin-top: 300rpx;
}