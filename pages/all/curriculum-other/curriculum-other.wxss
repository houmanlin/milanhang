page{
  background-color: #f3f3f3;
}
.nav {
  width: 100%;
  height: 170rpx;
  position: fixed;
  left: 0rpx;
  top: 0rpx;
  z-index: 9999;
}

.nav-clone {
  width: 100%;
  height: 170rpx;
}

.nav .nav-bgc {
  width: 100%;
  height: 100%;
}

.nav-logo {
  width:470rpx;
  height:100rpx;
  position:absolute;
  left:50%;
  top:64%;
  transform:translate(-50%, -50%);
}
.nav-back{
  position:absolute;
  left:0%;
  top:60%;
  transform:translateY(-50%);
  width:80rpx;
  height:80rpx;
  padding:20rpx;
  box-sizing:border-box;
}
.nav-back image{
  width: 100%;
  height: 100%;
}
.recruit{
  width: 100%;
}
/* 头部 */
.recruit-header{
  width: 100%;
  padding: 35rpx 0rpx 20rpx 0rpx;
  box-sizing: border-box;
  background-color: #fff;
}
.recruit-header-box{
  width: 90%;
  margin: 0 auto;
}
.header-box-image{
  width: 100%;
  border: 2rpx solid #fca760;
  border-radius: 20rpx;
  height: 450rpx;
  overflow: hidden;
  position: relative;
}
.header-box-image image{
  width: 100%;
  height: 100%;
}
.header-box-title{
  position: absolute;
  bottom: 0rpx;
  left: 0rpx;
  z-index: 100;
  width: 100%;
  text-align: center;
  height: 70rpx;
  background: -webkit-linear-gradient(left, #fe8e13 ,#dc9c34, #fad62e);
  background: -o-linear-gradient(right, #fe8e13, #dc9c34, #fad62e);
  background: -moz-linear-gradient(right, #fe8e13, #dc9c34, #fad62e);
  background: linear-gradient(to right, #fe8e13, #dc9c34, #fad62e);
  opacity: 0.5;
}
.header-box-real{
  position:absolute;
  bottom:3%;
  left:50%;
  transform:translateX(-50%);
  color:#ffffff;
  font-size:36rpx;
  z-index:200;
  text-shadow:0 2rpx rgb(254,143,16), 2rpx 0 rgb(254,143,16), -2rpx 0 rgb(254,143,16), 0 -2rpx rgb(254,143,16);
  width:100%;
  text-align:center;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.border{
  background: -webkit-linear-gradient(left, rgb(254,143,16) , rgb(249,187,29));
  background: -o-linear-gradient(right, rgb(254,143,16) , rgb(249,187,29));
  background: -moz-linear-gradient(right, rgb(254,143,16) , rgb(249,187,29));
  background: linear-gradient(to right, rgb(254,143,16) , rgb(249,187,29));
  width: 100%;
  height: 5rpx;
}
.header-box-content{
  margin-top: 20rpx;
  width: 100%;
}
.box-content-top{
  width: 100%;
  text-align: center;
  font-size: 28rpx;
  color: #fc9531;
  font-weight: bold;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.box-content-bottom{
  width: 100%;
  display: flex;
  justify-content: space-between;
  color: #a7a7a7;
  font-size: 26rpx;
  margin-top: 15rpx;
}
.content-bottom-right{
  color: #fc9531;
}
.content-bottom-right>text{
  color: #a7a7a7;
}

/* 中间部分 */
.recruit-content{
  width: 90%;
  margin: 0 auto;
  margin-top: 20rpx;
}
.recruit-content-top{
  width: 100%;
  position: relative;
}
.content-top-title{
  width: 22%;
  text-align: center;
  margin: 0 auto;
  color: #fc9531;
  font-size: 30rpx;
  border-radius: 10rpx 10rpx 0rpx 0rpx;
  border-top: 2rpx solid #fa9240;
  border-left: 2rpx solid #fa9240;
  border-right: 2rpx solid #fa9240;
  background-color: #fff;
  font-weight: bold;
  padding-top: 10rpx;
  box-sizing: border-box;
}
.top-small-border{
  width: 21%;
  background-color: #fff;
  height: 10rpx;
  position: absolute;
  left: 50%;
  bottom: -5rpx;
  transform: translateX(-50%);
}
.recruit-content-bottom{
  width: 100%;
  border: 2rpx solid #fa9240;
  border-radius: 10rpx;
  background-color: #fff;
  padding: 65rpx 0rpx 50rpx 50rpx;
  box-sizing: border-box;
  height: 680rpx;
  overflow: hidden;
}
.scroll{
  height: 100%;
  overflow-y: scroll;
  -webkit-overflow-scrolling: touch;
  display: flex;
  flex-wrap: wrap;
  width: 100%;
}
.content-bottom-li{
  width: 15%;
  height: 95rpx;
  border: 2rpx dashed #fe8e13;
  border-radius: 10rpx;
  margin-right: 9%;
  margin-bottom: 9%;
}
.content-bottom-li image{
  width: 100%;
  height: 100%;
}

/* 底部详情 */
.recruit-bottom{
  /* margin-top: 20rpx; */
  width: 100%;
  background-color: #fff;
}
.recruit-bottom-top{
  width: 100%;
  padding: 20rpx 0rpx;
}
.bottom-top-box{
  width: 90%;
  margin: 0 auto;
}
.top-box-image{
  border-radius: 10rpx;
  overflow: hidden;
  width: 100%;
  height: 300rpx;
  position: relative;
}
video{
  width: 100%;
  height: 100%;
}
.video-cover{
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
}
.video-play-btn{
  position: absolute;
  width: 100rpx;
  height: 100rpx;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  margin: auto;
}
.top-box-title{
  width: 90%;
  margin: 0 auto;
  height: 50rpx;
  line-height: 29rpx;
  color: #fff;
  font-size: 28rpx;
  margin-top: 10rpx;
  margin-bottom: 20rpx;
  padding: 10rpx;
  box-sizing: border-box;
  background: url("data:image/png;base64,");
  background-repeat: no-repeat;
  /* background-size: contain; */
}
.top-box-detail{
  font-size: 24rpx;
  color: #323232;
  line-height: 30rpx;
  padding-left: 10rpx;
  margin-bottom: 30rpx;
}
.top-box-detail-title{
  margin-top: 20rpx;
}
.recruit-bottom-bottom-clone{
  width: 100%;
  height: 85rpx;
}
.recruit-bottom-bottom{
  width: 100%;
  position: fixed;
  left: 0rpx;
  bottom: 0rpx;
  background-color: #f3f3f3;
}
.bottom-bottom-detail{
  width: 100%;
  display: flex;
  justify-content: space-between;
}
.bottom-detail-left{
  width: 70%;
  padding: 20rpx 0rpx 0rpx 20rpx;
  box-sizing: border-box;
  color: black;
  font-weight: bold;
  font-size: 32rpx;
  display: flex;
}
.afterPrice{
  color: #323232;
}
.afterPrice>text{
  color: #fe8e13;
  font-size: 36rpx;
}
.beforePrice{
  color:#a7a7a7;
  font-size:24rpx;
  margin-top:8rpx;
  margin-left:10rpx;
}
.bottom-detail-right{
  width: 30%;
  height: 80rpx;
}
.bottom-detail-right image{
  width: 100%;
  height: 100%;
}
.allImage{
  width: 90%;
  margin: 0 auto;
  text-align: center;
  overflow: hidden;
  margin-bottom: 10rpx;
}